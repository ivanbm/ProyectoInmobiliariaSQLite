<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.izv.android.proyectoinmobiliaria" >
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <provider android:name=".Proveedor"
            android:exported="true"
            android:enabled="true"
            android:authorities="com.izv.android.proyectoinmobiliaria"> </provider>
        <activity
            android:name="com.izv.android.proyectoinmobiliaria.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Secundaria"
            android:label="@string/title_activity_secundaria" >
        </activity>
        <activity
            android:name=".Anadir"
            android:label="@string/anadir" >
        </activity>
    </application>

</manifest>
